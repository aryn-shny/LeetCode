class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]: #Return type is a list
        num_map = {} #Declaring a dictionary

        for i in range(len(nums)): 
            complement = target - nums[i] 

            if complement in nums: 
                return num_map[num_map[complement],i] #Return a list [index of (target-number1),number1]
        
        num_map[nums[i]] = i #number:index
